   "source": [
    "a=float(input('Enter side a of traingle: '))\n",
    "b=float(input('Enter side b of traingle: '))\n",
    "c=float(input('Enter side  c of traingle: '))\n",
    "\n",
    "# semi-perimeter\n",
    "\n",
    "s=(a+b+c)/2\n",
    "\n",
    "#area\n",
    "\n",
    "area=(s*(s-a)*(s-b)*(s-c))**0.5\n",
    "\n",
    "print(\"Area of Triangle is : %0.2f\" %area)"
